{ 
"manifest_version": 2, 
"name": "google-search-result-formatter", 
"description": "Put title above URL", 
"version": "0.1", 
"author": "haijin",
"browser_action": {
      "default_icon": "formatter.png"
    },
"background": {
  "scripts": []
},  
 "content_scripts": [
    {
      "matches": [
        "https://www.google.com/search?*"
      ],
	  "run_at": "document_start",
	  "all_frames": true,
      "js": ["resultformatter.js"]
    }
  ],
"permissions": ["activeTab"]}


