{ 
"manifest_version": 2, 
"name": "google-search-result-formatter", 
"description": "Put title above URL", 
"version": "1.1", 
"author": "Haijin",
"browser_action": {
      "default_icon": "formatter.png"
    },
"background": {
  "scripts": []
},  
"content_scripts": [
    {
      "matches":[
	  "https://www.google.com/search?*", 
	  "https://www.google.co.uk/search?*",
	  "https://www.google.co.th/search?*",
	  "https://www.google.co.in/search?*",
	  "https://www.google.co.jp/search?*",
	  ],
	  "run_at": "document_start",
	  "all_frames": true,
      "js": ["resultformatter.js"]
    }
  ]
}


